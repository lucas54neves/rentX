{
  "name": "rentx",
  "version": "1.0.0",
  "main": "src/shared/infra/http/server.ts",
  "repository": "https://github.com/lucas54neves/rentx.git",
  "author": "Lucas Neves <lucas54neves@outlook.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/shared/infra/http/server.ts",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "test:sonar": "NODE_ENV=test jest --coverage --testResultsProcessor=jest-sonar-reporter --detectOpenHandles",
    "test": "NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:watch": "yarn jest --runInBand --watch -f",
    "seed:admin": "ts-node-dev src/shared/infra/typeorm/seed/admin.ts"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "csv-parse": "^4.15.4",
    "dayjs": "^1.10.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6",
    "tsyringe": "^4.5.0",
    "typeorm": "^0.2.32",
    "supertest": "^6.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@lucas54neves/prettier-config": "^1.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/multer": "^1.4.5",
    "@types/node": "^15.0.2",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "jest": "^27.0.3",
    "jest-sonar-reporter": "^2.0.0",
    "ts-jest": "^27.0.2",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "prettier": "@lucas54neves/prettier-config",
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "testResultsProcessor": "jest-sonar-reporter",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
